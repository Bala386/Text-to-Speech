    "\n",
    "\n",
    "from gtts import gTTS\n",
    "import os\n",
    "\n",
    "text = \"Hello! Welcome to my Text to Speech project using Python and NLP.\"\n",
    "\n",
    "language = 'en'\n",
    "\n",
    "tts = gTTS(text=text, lang=language, slow=False)\n",
    "\n",
    "tts.save(\"output.mp3\")\n",
    "os.system(\"start output.mp3\")  \n"
   ]
  }
